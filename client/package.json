    "raf": "3.4.0",
    "rc-pagination": "^1.16.3",
    "react": "^16.3.0",
    "react-dev-utils": "^4.2.1",
    "react-device-detect": "^1.6.1",
    "react-dom": "^16.2.0",
    "react-infinite-scroller": "^1.1.4",
